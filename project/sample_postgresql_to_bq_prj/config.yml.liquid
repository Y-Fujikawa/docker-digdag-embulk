in:
  type: postgresql
  host: {{ env.pg_host }}
  user: {{ env.pg_user }}
  password: {{ env.pg_password }}
  database: {{ env.pg_database }}
  query: |
    select id, name from employee
out:
  type: bigquery
  auth_method: json_key
  json_keyfile: {{ env.json_keyfile }}
  path_prefix: /tmp/ # 一時ファイル作成場所
  project: {{ env.bq_project }}
  dataset: {{ env.dataset }}
  table: {{ env.table }}
